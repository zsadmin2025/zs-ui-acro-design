{
  "name": "zs-arco-design-vue",
  "description": "zs",
  "version": "1.0.0",
  "private": true,
  "author": "zs",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config ./config/vite.config.dev.ts",
    "prd": "vite --config ./config/vite.config.prod.ts",
    "build": "vue-tsc --noEmit && vite build --config ./config/vite.config.dev.ts",
    "build:prod": "vue-tsc --noEmit && NODE_OPTIONS=--max-old-space-size=8192 vite build --config ./config/vite.config.prod.ts",
    "report": "cross-env REPORT=true npm run build",
    "preview": "npm run build && vite preview --host",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint-staged": "npx lint-staged",
    "prepare": "husky install",
    "lint-fix": "eslint --fix --ext .js --ext .vue src/"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {

    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "decimal.js": "^10.6.0",
    "echarts": "^5.6.0",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "qs": "^6.14.0",
